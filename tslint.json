{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-console": [
            false
        ],
        "semicolon": [
            false
        ],
        "quotemark": [
            false
        ],
        "object-literal-sor t-keys": [
            false
        ],
        "max-classes-per-file": [
            false
        ]
    },
    "rulesDirectory": []
}